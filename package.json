{
  "name": "spoktor",
  "version": "1.0.0",
  "description": "An spotify to traktor playlist converter",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "deploy": "./deploy.sh",
    "build": "rollup --config",
    "serve": "web-dev-server --watch",
    "test": "uvu --ignore '\\.d\\.ts'",
    "test:watch": "watchlist src tests -- npm run test",
    "test:unit": "npm run test -- tests/unit",
    "test:unit:watch": "watchlist src tests -- npm run test:unit",
    "test:e2e": "npm run test -- tests/e2e",
    "update-dependencies": "npx npm-upgrade"
  },
  "author": "Ferran Llop",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-terser": "0.4.4",
    "@web/dev-server": "0.4.6",
    "puppeteer": "23.2.1",
    "puppeteer-screenshot-tester": "1.6.0",
    "rollup": "4.21.1",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-delete": "2.0.0",
    "uvu": "0.5.6",
    "watchlist": "0.3.1"
  },
  "dependencies": {
    "ramda": "0.30.1"
  }
}
